{
  "compilerOptions": {
    "strict": false,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "allowJs": true,
    "checkJs": true
  },
  "include": ["*.ts", "*.tsx"]
}
